{
  "name": "sequence-collab",
  "version": "0.0.0",
  "private": true,
  "homepage": "http://telepromptu.com/",
  "build": {
    "appId": "com.electron.electron-with-create-react-app",
    "win": {
      "iconUrl": "https://cdn2.iconfinder.com/data/icons/designer-skills/128/react-256.png"
    },
    "directories": {
      "buildResources": "public"
    }
  },
  "main": "src/start.js",
  "scripts": {
    "start": "react-scripts start",
    "react-start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "electron-start": "nf start -p 3000",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-mocha **/*.spec.ts",
    "electron": "electron .",
    "do-electron-start": "node src/start-react",
    "pack": "build --dir",
    "dist": "npm run build && build",
    "server": "TS_NODE_PROJECT=tsconfig.server.json DEBUG=sj:* ts-node-dev src/server/index.ts",
    "server-pm2": "pm2 start npm --no-automation --name telepromptu-server -- run server",
    "deploy-client": "npm run build && scp -r ./build/* geof@nodedroplet:/home/geof/webroot/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "## usage",
  "repository": {
    "type": "git",
    "url": "https://github.com/geofholbrook/sequence-collab.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@geof/socket-messaging": "^0.4.1",
    "@musicenviro/base": "^0.2.6",
    "@musicenviro/ui-elements": "^0.4.18",
    "@types/cors": "^2.8.6",
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.149",
    "@types/react-beforeunload": "^2.1.0",
    "@types/react-facebook-login": "^4.1.1",
    "@types/superagent": "^4.1.7",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "electron": "^8.2.0",
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-beforeunload": "^2.1.0",
    "react-dom": "^16.13.1",
    "react-facebook-login": "^4.1.1",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "semantic-ui-react": "^0.88.2",
    "styled-components": "^5.1.0",
    "superagent": "^5.2.2",
    "tone": "^14.5.45",
    "ts-node": "^8.8.2",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.6",
    "@types/jest": "^24.9.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.12.32",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "chai": "^4.2.0",
    "mocha": "^7.1.1",
    "prettier": "^2.0.2",
    "ts-mocha": "^7.0.0",
    "ts-node-dev": "^1.0.0-pre.44"
  }
}
