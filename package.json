{
  "name": "sequence-collab",
  "version": "0.0.0",
  "private": true,
  "homepage": "http://telepromptu.com",
  "build": {
    "appId": "com.electron.electron-with-create-react-app",
    "win": {
      "iconUrl": "https://cdn2.iconfinder.com/data/icons/designer-skills/128/react-256.png"
    },
    "directories": {
      "buildResources": "public"
    }
  },
  "main": "src/start.js",
  "scripts": {
    "start": "node scripts/start.js",
    "react-start": "BROWSER=none node scripts/start.js",
    "build": "node scripts/build.js",
    "electron-start": "nf start -p 3000",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-mocha **/*.spec.ts",
    "electron": "electron .",
    "do-electron-start": "node src/start-react",
    "pack": "build --dir",
    "dist": "npm run build && build",
    "server": "TS_NODE_PROJECT=tsconfig.commonjs.json DEBUG=sj:*,gsm:* ts-node-dev src/server/index.ts",
    "server-pm2": "pm2 start npm --no-automation --name telepromptu-server -- run server",
    "deploy-client": "npm run build && scp -r ./build/* geof@nodedroplet:/home/geof/webroot/",
    "deploy-server": "tsc --project tsconfig.server.json && scp -r ./serverjs/* geof@nodedroplet:/home/geof/jammr-server/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "## usage",
  "repository": {
    "type": "git",
    "url": "https://github.com/geofholbrook/sequence-collab.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "7.9.0",
    "@geof/socket-messaging": "^0.5.4",
    "@musicenviro/base": "^0.2.8",
    "@musicenviro/ui-elements": "file:../musicenviro__ui-elements",
    "@svgr/webpack": "4.3.3",
    "@types/cors": "^2.8.6",
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.149",
    "@types/react-beforeunload": "^2.1.0",
    "@types/react-facebook-login": "^4.1.1",
    "@types/react-redux": "^7.1.7",
    "@types/superagent": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.1.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.2",
    "camelcase": "^5.3.1",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "cors": "^2.8.5",
    "css-loader": "3.4.2",
    "debug": "^4.1.1",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "electron": "^8.2.0",
    "eslint": "^6.6.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "express": "^4.17.1",
    "file-loader": "4.3.0",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "4.0.0-beta.11",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.2",
    "mini-css-extract-plugin": "0.9.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.6.4",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-beforeunload": "^2.1.0",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.1",
    "react-facebook-login": "^4.1.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "resolve": "1.15.0",
    "resolve-url-loader": "3.1.1",
    "sass-loader": "8.0.2",
    "semantic-ui-react": "^0.88.2",
    "semver": "6.3.0",
    "style-loader": "0.23.1",
    "styled-components": "^5.1.0",
    "superagent": "^5.2.2",
    "terser-webpack-plugin": "2.3.5",
    "tone": "^14.5.45",
    "ts-node": "^8.8.2",
    "ts-pnp": "1.1.6",
    "typescript": "^3.7.5",
    "url-loader": "2.3.0",
    "webpack": "4.42.0",
    "webpack-dev-server": "3.10.3",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "4.3.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.6",
    "@types/jest": "^24.9.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.12.32",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "chai": "^4.2.0",
    "mocha": "^7.1.1",
    "prettier": "^2.0.2",
    "ts-mocha": "^7.0.0",
    "ts-node-dev": "^1.0.0-pre.44"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
