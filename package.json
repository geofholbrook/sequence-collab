  "private": true,
  "homepage": "http://geofholbrook.com/programming/music-enviro/sequence-collab/build",
  "build": {
    "appId": "com.electron.electron-with-create-react-app",
    "win": {
      "iconUrl": "https://cdn2.iconfinder.com/data/icons/designer-skills/128/react-256.png"
    },
    "directories": {
      "buildResources": "public"
    }
  },
  "main": "src/start.js",
  "dependencies": {
    "@geof/socket-messaging": "^0.1.4",
    "@musicenviro/base": "^0.2.1",
    "@musicenviro/ui-elements": "^0.4.5",
    "electron": "^8.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/jest": "^24.9.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.12.32",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "chai": "^4.2.0",
    "mocha": "^7.1.1",
    "prettier": "^2.0.2",
    "ts-mocha": "^7.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "react-start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "electron-start": "nf start -p 3000",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-mocha **/*.spec.ts",
    "electron": "electron .",
    "do-electron-start": "node src/start-react",
    "pack": "build --dir",
    "dist": "npm run build && build",
    "server": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node src/server/index.ts"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "## usage",
  "repository": {
    "type": "git",
